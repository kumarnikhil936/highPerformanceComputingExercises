default: compile_new run_new

compile:
	gcc -g -Wall -pthread -o pi pi.c 

run:
	./pi 1 42949672 
	./pi 2 42949672 
	./pi 4 42949672 
	./pi 8 42949672 
	./pi 16 42949672 
	./pi 32 42949672 

compile_new:
	gcc -g -Wall -pthread -o pi1 pi1.c

run_new:
	./pi1 1 4294967296 
	./pi1 2 4294967296 
	./pi1 4 4294967296 
	./pi1 8 4294967296 
	./pi1 16 4294967296 
	./pi1 32 4294967296 
